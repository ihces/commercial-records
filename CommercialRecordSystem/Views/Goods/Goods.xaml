                <ComboBoxItem>Markaya Göre Sıralı</ComboBoxItem>
                <ComboBoxItem>Kategoriye Göre Sıralı</ComboBoxItem>
            </ComboBox>
            <SearchBox Grid.Column="3" HorizontalAlignment="Right" FontSize="24"  Width="310" BorderThickness="0" VerticalAlignment="Bottom" Margin="0,0,40,40" />
